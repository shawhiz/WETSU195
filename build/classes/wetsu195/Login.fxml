<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="600.0" prefWidth="600.0" stylesheets="@Resources/wetsu.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wetsu195.LoginController">
    <children>
        <Button fx:id="login" defaultButton="true" layoutX="246.0" layoutY="495.0" onAction="#Login" prefHeight="39.0" prefWidth="109.0" text="Login" textAlignment="CENTER" textFill="WHITE" />
      <PasswordField fx:id="password" layoutX="198.0" layoutY="427.0" promptText="Password">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></PasswordField>
      <TextField fx:id="username" layoutX="198.0" layoutY="373.0" promptText="Username">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></TextField>
      <ImageView fx:id="logo" fitHeight="325.0" fitWidth="325.0" layoutX="138.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Resources/WETSU_large.png" />
         </image>
      </ImageView>
      <ChoiceBox fx:id="locale" layoutX="14.0" layoutY="549.0" prefWidth="150.0" styleClass="button-date" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" />
    </children>
</AnchorPane>
